<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-responsive.css" rel="stylesheet" />
    <link href="~/Content/jquery.pnotify.css" rel="stylesheet" />
    <link href="~/Content/jquery.pnotify.icons.css" rel="stylesheet" />
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
    
</head>
<body>

    <div class="container body-content">
        @RenderBody()
    </div>
    <script src="~/Scripts/jquery-2.1.1.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/jquery.pnotify.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/Scripts/DataTables-1.10.3/jquery.dataTables.js"></script>
    <script src="~/Scripts/DataTables-1.10.3/dataTables.tableTools.js"></script>
    <script src="~/Scripts/DataTables-1.10.3/dataTables.scroller.js"></script>
    <script src="~/Scripts/DataTables-1.10.3/dataTables.responsive.js"></script>
    <script src="~/Scripts/DataTables-1.10.3/dataTables.keyTable.js"></script>
    <script src="~/Scripts/DataTables-1.10.3/dataTables.foundation.js"></script>
    <script src="~/Scripts/DataTables-1.10.3/dataTables.fixedHeader.js"></script>
    <script src="~/Scripts/DataTables-1.10.3/dataTables.fixedColumns.js"></script>
    <script src="~/Scripts/DataTables-1.10.3/dataTables.colVis.js"></script>
    <script src="~/Scripts/DataTables-1.10.3/dataTables.colReorder.js"></script>
    <script src="~/Scripts/DataTables-1.10.3/dataTables.bootstrap.js"></script>
    <script src="~/Scripts/DataTables-1.10.3/dataTables.autoFill.js"></script>
    @RenderSection("InLineJavaScript",false)
    
</body>
</html>